{
  "name": "s1lver/yii2-summernote",
  "description": "Summernote Super Simple WYSIWYG Editor for Yii2",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0|^8.1",
    "yiisoft/yii2": "^2.0.45",
    "bower-asset/summernote": "@stable"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^4.29.0"
  },
  "autoload": {
    "psr-4": {
      "s1lver\\summernote\\": "src"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
