{
  "name": "s1lver/yii2-summernote",
  "description": "Summernote Super Simple WYSIWYG Editor for Yii2",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0|^8.1",
    "yiisoft/yii2": "^2.0.45",
    "bower-asset/summernote": "@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "roave/infection-static-analysis-plugin": "^1.16",
    "vimeo/psalm": "^4.30|^5.8"
  },
  "autoload": {
    "psr-4": {
      "s1lver\\summernote\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "S1lver\\Summernote\\Tests\\": "tests"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "infection/extension-installer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
